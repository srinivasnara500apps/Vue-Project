        "@nuxt/schema": "^3.5.2",
        "@nuxt/test-utils": "^3.5.1",
        "@nuxtjs/eslint-config-typescript": "^12.0.0",
        "@nuxtjs/tailwindcss": "^6.8.0",
        "@types/node": "^18",
        "@typescript-eslint/eslint-plugin": "^6.5.0",
        "@typescript-eslint/parser": "^6.5.0",
        "@vue/cli-service": "^5.0.8",
        "@vue/compiler-sfc": "^3.3.4",
        "babel-loader": "^9.1.3",
        "changelogen": "^0.5.3",
